{
  "compilerOptions": {
    "sourceMap": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "strictPropertyInitialization": false
  },
  "exclude": [
    "node_modules"
  ]
}
